<?xml version="1.0"?>  
<robot name="box_bot" xmlns:xacro="http://www.ros.org/wiki/xacro">
        
  <link name="base_link"/>

  <joint name="j_phantomx_attachment" type="fixed">
    <parent link="base_link"/>
    <child link="BASE_LIMB"/>
    <origin rpy="0 0 0" xyz="0 0 0.2"/>
  </joint>

  <link name="BASE_LIMB">
    <inertial>
      <origin xyz="0 0 -0.125"/>
      <mass value="10"/>
      <inertia ixx="0.0854" ixy="0.0" ixz="0.0" iyy="0.0854" iyz="0.0" izz="0.0666"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 -0.125"/>
      <geometry>
        <box size="0.2 0.2 0.25"/>
      </geometry>
      <material name="Grey">
        <color rgba="0.20 0.20 0.2 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 -0.125"/>
      <geometry>
        <box size="0.2 0.2 0.25"/>a
      </geometry>
    </collision>
  </link>

  <link name="c1_rf">
    <inertial>
      <origin rpy="0 0 0" xyz="0 -0.02633 0"/>
      <mass value="0.024357719"/>
      <inertia ixx="0.0051411124" ixy="-0.00057530255" ixz="-0.000024729049" iyy="0.0081915737" iyz="-0.000019223094" izz="0.0011379812"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 -0.0252 0"/>
      <geometry>
        <!--box size="0.045 0.022 0.0325" /-->
        <mesh filename="package://moonbot_description/meshes_phantom/connect.STL" scale="1 1 1"/>
      </geometry>
      <material name="Grey">
        <color rgba="0.20 0.20 0.20 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 -0.02633 0"/>
      <geometry>
        <mesh filename="package://moonbot_description/meshes_phantom/connect_coll.STL" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>

  <link name="c2_rf">
    <inertial>
      <origin rpy="0 0 0" xyz="0 -0.02633 0"/>
      <mass value="0.024357719"/>
      <inertia ixx="0.0051411124" ixy="-0.00057530255" ixz="-0.000024729049" iyy="0.0081915737" iyz="-0.000019223094" izz="0.0011379812"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 -0.0252 0"/>
      <geometry>
        <!--box size="0.045 0.022 0.0325" /-->
        <mesh filename="package://moonbot_description/meshes_phantom/connect.STL" scale="1 1 1"/>
      </geometry>
      <material name="Grey">
        <color rgba="0.20 0.20 0.20 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 -0.02633 0"/>
      <geometry>
        <mesh filename="package://moonbot_description/meshes_phantom/connect_coll.STL" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>

  <link name="thigh_rf">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.024357719"/>
      <inertia ixx="0.0051411124" ixy="-0.00057530255" ixz="-0.000024729049" iyy="0.0081915737" iyz="-0.000019223094" izz="0.0011379812"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <!--box size="0.045 0.022 0.0325" /-->
        <mesh filename="package://moonbot_description/meshes_phantom/thigh_l.STL" scale="1 1 1"/>
      </geometry>
      <material name="Grey">
        <color rgba="0.20 0.20 0.20 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://moonbot_description/meshes_phantom/thigh_l_coll.STL" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>

  <link name="tibia_rf">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.024357719"/>
      <inertia ixx="0.0051411124" ixy="-0.00057530255" ixz="-0.000024729049" iyy="0.0081915737" iyz="-0.000019223094" izz="0.0011379812"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <!--box size="0.045 0.022 0.0325" /-->
        <mesh filename="package://moonbot_description/meshes_phantom/tibia_l.STL" scale="1 1 1"/>
      </geometry>
      <material name="Grey">
        <color rgba="0.20 0.20 0.20 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://moonbot_description/meshes_phantom/tibia_l_coll.STL" scale="1 1 1"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0.1</mu>
            <mu2>0.1</mu2>
          </ode>
        </friction>
      </surface>
    </collision>
  </link>

  <joint name="j_c1_rf" type="revolute">
    <parent link="BASE_LIMB"/>
    <child link="c1_rf"/>
    <origin rpy="0.785 4.7123 0.7853981633974483" xyz="0.1 0 -0.05"/>
    <axis xyz="1 0 0"/>
    <limit effort="2.8" lower="-2.6179939" upper="2.6179939" velocity="5.6548668"/>
  </joint>

  <joint name="j_c2_rf" type="fixed">
    <parent link="c1_rf"/>
    <child link="c2_rf"/>
    <origin rpy="0 1.5704 3.14159" xyz="0 -0.054 0"/>
  </joint>

  <joint name="j_thigh_rf" type="revolute">
    <parent link="c2_rf"/>
    <child link="thigh_rf"/>
    <!--origin xyz="0 0 0" rpy=" 0 3.14159 3.14159" /-->
    <origin rpy="3.14159 0  0" xyz="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit effort="2.8" lower="-2.6179939" upper="2.6179939" velocity="5.6548668"/>
  </joint>

  <joint name="j_tibia_rf" type="revolute">
    <parent link="thigh_rf"/>
    <child link="tibia_rf"/>
    <origin rpy="-1.5707 0 3.14159" xyz="0 -0.0645 -0.0145"/>
    <axis xyz="1 0 0"/>
    <limit effort="2.8" lower="-2.6179939" upper="2.6179939" velocity="5.6548668"/>
  </joint>

  <gazebo reference="c1_rf">
    <material>Gazebo/Grey</material>
  </gazebo>
  <gazebo reference="c2_rf">
    <material>Gazebo/Grey</material>
  </gazebo>
  <gazebo reference="thigh_rf">
    <material>Gazebo/Grey</material>
  </gazebo>
  <gazebo reference="tibia_rf">
    <material>Gazebo/Grey</material>
  </gazebo>

  <ros2_control name="MyRobotSystem" type="system">
    <hardware>
      <plugin>gazebo_ros2_control/GazeboSystem</plugin>
    </hardware>
    
    <joint name="j_c1_rf">
      <command_interface name="position">
        <param name="min">-2.6179939</param>
        <param name="max">2.6179939</param>
      </command_interface>
      <command_interface name="velocity">
        <param name="min">-5.6548668</param>
        <param name="max">5.6548668</param>
      </command_interface>
      <command_interface name="effort">
        <param name="min">-2.8</param>
        <param name="max">2.8</param>
      </command_interface>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
      <state_interface name="effort"/>
    </joint>

    <joint name="j_thigh_rf">
      <command_interface name="position">
        <param name="min">-2.6179939</param>
        <param name="max">2.6179939</param>
      </command_interface>
      <command_interface name="velocity">
        <param name="min">-5.6548668</param>
        <param name="max">5.6548668</param>
      </command_interface>
      <command_interface name="effort">
        <param name="min">-2.8</param>
        <param name="max">2.8</param>
      </command_interface>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
      <state_interface name="effort"/>
    </joint>

    <joint name="j_tibia_rf">
      <command_interface name="position">
        <param name="min">-2.6179939</param>
        <param name="max">2.6179939</param>
      </command_interface>
      <command_interface name="velocity">
        <param name="min">-5.6548668</param>
        <param name="max">5.6548668</param>
      </command_interface>
      <command_interface name="effort">
        <param name="min">-2.8</param>
        <param name="max">2.8</param>
      </command_interface>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
      <state_interface name="effort"/>
    </joint>
  </ros2_control>

  <!-- Gazebo's ros2_control plugin  -->
  <gazebo>
    <plugin filename="libgazebo_ros2_control.so" name="gazebo_ros2_control">
      <!-- <robotNamespace>/phantomx</robotNamespace> -->
      <robot_sim_type>gazebo_ros2_control/GazeboSystem</robot_sim_type>
      <robot_param>robot_description</robot_param>
      <robot_param_node>robot_state_publisher_node</robot_param_node>
      <parameters>$(find moonbot_control)/config/single_config.yaml</parameters>
    </plugin>
  </gazebo>  
   
</robot>

